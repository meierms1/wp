{
  "name": "maycon-portfolio-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.0",
    "clsx": "^1.2.0",
    "date-fns": "^2.29.0",
    "framer-motion": "^9.0.0",
    "plotly.js": "^2.18.0",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.7.0",
    "react-intersection-observer": "^9.4.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "react-type-animation": "^2.1.2",
    "tailwindcss": "^3.2.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k -n flask,react -c green,cyan \"FLASK_APP=../app.py FLASK_DEBUG=1 flask run --port 5000\" \"npm start\""
  },
  "sideEffects": [
    "*.css",
    "src/index.css"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}
